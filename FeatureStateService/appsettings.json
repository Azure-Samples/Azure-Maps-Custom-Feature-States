{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },

  "Styles": {
    "StyleRules": [
      {
        "FeatureState": "free",
        "Color": "green"
      },
      {
        "FeatureState": "occupied",
        "Color": "red"
      }
    ]
  },

  "FeatureDownloader": {
    "AllowUseLocalCache": true
  },

  "AllowedHosts": "*"
}
